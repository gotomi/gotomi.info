doctype html
html(lang="en")
    head
        meta(charset='utf-8')
        title Tomasz Gospodarczyk | front-end developer
        meta(name='viewport', content='width=device-width, initial-scale=1')
        meta(name='theme-color' content='#000')
        style
            include parts/style.css
    body
        include parts/content.html
        ul.photos
            each item in feed
                each image in item.images
                    li
                        a(href=item.link)
                            img(src='images/' + image.filename , alt=item.text)
    footer
        div
            p &copy; 2018 Tomasz Gospodarczyk | designed in the browser




